    "@supabase/supabase-js": "^2.56.0",
    "@types/dompurify": "^3.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vercel/node": "^3.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "cheerio": "^1.1.2",
    "dompurify": "^3.2.6",
    "express-rate-limit": "^8.0.1",
    "groq-sdk": "^0.30.0",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.2",
    "lucide-react": "^0.542.0",
    "openai": "^5.12.2",
    "postcss": "^8.4.38",
    "puppeteer": "^24.17.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "rss-parser": "^3.13.0",
    "stripe": "^18.4.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2",
    "vite": "^5.3.1"
