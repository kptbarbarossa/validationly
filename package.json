{
  "name": "validationly-vite",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode production",
    "build:safe": "SKIP_TYPE_CHECK=true vite build",
    "build:api": "node build-api.js",
    "build:all": "npm run build && npm run build:api",
    "preview": "vite preview",
