    "test:youtube": "node test-youtube.js",
    "test:supabase": "node test-supabase.js",
    "type-check": "tsc --noEmit && tsc --project tsconfig.api.json --noEmit",
    "type-check:api": "tsc --project tsconfig.api.json --noEmit",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@google/genai": "^1.11.0",
    "@google/generative-ai": "^0.24.1",
    "@react-pdf/renderer": "^4.3.0",
