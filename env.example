# Google Identity Service Configuration
# Copy this file to .env.local and fill in your actual values

# Google OAuth 2.0 Client ID for One Tap Sign-In
VITE_GOOGLE_CLIENT_ID=your_google_oauth_client_id_here

# Google OAuth 2.0 Client Secret (if needed)
VITE_GOOGLE_CLIENT_SECRET=your_google_oauth_client_secret_here

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# Google API Key (used for Gemini AI)
GOOGLE_API_KEY=your_google_api_key

# Google Gemini API Key (alternative, GOOGLE_API_KEY preferred)
GOOGLE_GENAI_API_KEY=your_google_gemini_api_key

# Hugging Face API Key for AI Image Generation
HUGGINGFACE_API_KEY=your_huggingface_api_key

# JWT Secret
JWT_SECRET=your_jwt_secret_here

# Other API Keys
REDDIT_CLIENT_ID=your_reddit_client_id
GITHUB_ACCESS_TOKEN=your_github_access_token
PRODUCTHUNT_ACCESS_TOKEN=your_producthunt_access_token
